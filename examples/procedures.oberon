MODULE Procedures;
VAR x, y, sum: INTEGER;

PROCEDURE Add(a, b: INTEGER);
BEGIN
    sum := a + b;
END Add;

PROCEDURE PrintMessage(msg: STRING);
BEGIN
    Write(msg);
    WriteLn();
END PrintMessage;

BEGIN
    x := 5;
    y := 3;
    Add(x, y);
    Write("Sum of ");
    Write(x);
    Write(" and ");
    Write(y);
    Write(" is ");
    Write(sum);
    WriteLn();
    
    PrintMessage("Procedure call successful!");
END Procedures.
