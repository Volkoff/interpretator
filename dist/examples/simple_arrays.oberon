MODULE SimpleArrays;
VAR numbers: ARRAY[5] OF INTEGER;
    i, sum: INTEGER;

BEGIN
    numbers[0] := 1;
    numbers[1] := 2;
    numbers[2] := 3;
    numbers[3] := 4;
    numbers[4] := 5;
    
    sum := 0;
    FOR i := 0 TO 4 DO
        sum := sum + numbers[i];
    END;
    
    Write("Sum of array elements: ");
    Write(sum);
    WriteLn();
END SimpleArrays.
